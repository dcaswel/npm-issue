{
  "name": "client",
  "version": "1.0.0",
  "description": "GoReact Client Files",
  "scripts": {
    "build": "webpack --progress",
    "serve": "webpack-dev-server --watch --hot --https --host 0.0.0.0 --page",
    "test": "karma start",
    "eslint:dashboard": "eslint 'dashboard/**/*.ts'",
    "eslint:go-modules": "eslint 'go-modules/src/**/*.ts'",
    "eslint:lti": "eslint 'lti/**/*.ts'",
    "test:dashboard": "RUN_TARGET=test karma start --project dashboard",
    "test:ngx-dashboard": "RUN_TARGET=test karma start --project ngx-dashboard",
    "test:go-modules-al": "RUN_TARGET=test karma start --project go-modules-al",
    "test:go-modules-m": "RUN_TARGET=test karma start --project go-modules-m",
    "test:go-modules-nz": "RUN_TARGET=test karma start --project go-modules-nz",
    "test:ngx-go-modules": "RUN_TARGET=test karma start --project ngx-go-modules",
    "test:lti": "RUN_TARGET=test karma start --project lti",
    "test:ngx-lti": "RUN_TARGET=test karma start --project ngx-lti",
    "test:dashboard:ci": "yarn run test:dashboard --coverage --ci",
    "test:ngx-dashboard:ci": "yarn run test:ngx-dashboard --coverage --ci",
    "test:go-modules-al:ci": "yarn run test:go-modules-al --coverage --ci",
    "test:go-modules-m:ci": "yarn run test:go-modules-m --coverage --ci",
    "test:go-modules-nz:ci": "yarn run test:go-modules-nz --coverage --ci",
    "test:ngx-go-modules:ci": "yarn run test:ngx-go-modules --coverage --ci",
    "test:lti:ci": "yarn test:lti --coverage --ci",
    "test:ngx-lti:ci": "yarn test:ngx-lti --coverage --ci",
    "test:all": "yarn test:dashboard && yarn test:go-modules && yarn test:lti && yarn test:ngx-dashboard && yarn test:ngx-go-modules && yarn test:ngx-lti",
    "report-coverage:dashboard": "comforter-cli --path ./dashboard/.coverage/lcov.info --name dashboard --branch $CI_COMMIT_REF_NAME --commit $CI_COMMIT_SHA ${CI_MERGE_REQUEST_IID:+--merge-request-iid $CI_MERGE_REQUEST_IID} ${CI_MERGE_REQUEST_DIFF_BASE_SHA:+--merge-base $CI_MERGE_REQUEST_DIFF_BASE_SHA} ${CI_MERGE_REQUEST_TARGET_BRANCH_NAME:+--target-branch $CI_MERGE_REQUEST_TARGET_BRANCH_NAME} --project $CI_PROJECT_ID --apiKey $GITLAB_API_KEY --zip ./dashboard/.coverage/lcov-report --host https://comforter.goreact.com",
    "report-coverage:ngx-dashboard": "comforter-cli --path ./angular/dashboard/.coverage/lcov.info --name ngx-dashboard --branch $CI_COMMIT_REF_NAME --commit $CI_COMMIT_SHA ${CI_MERGE_REQUEST_IID:+--merge-request-iid $CI_MERGE_REQUEST_IID} ${CI_MERGE_REQUEST_DIFF_BASE_SHA:+--merge-base $CI_MERGE_REQUEST_DIFF_BASE_SHA} ${CI_MERGE_REQUEST_TARGET_BRANCH_NAME:+--target-branch $CI_MERGE_REQUEST_TARGET_BRANCH_NAME} --project $CI_PROJECT_ID --apiKey $GITLAB_API_KEY --zip ./angular/dashboard/.coverage/lcov-report --host https://comforter.goreact.com",
    "merge-coverage:go-modules": "move-file go-modules/.coverage-al/coverage-final.json .nyc_output/coverage-al.json && move-file go-modules/.coverage-nz/coverage-final.json .nyc_output/coverage-nz.json && move-file go-modules/.coverage-m/coverage-final.json .nyc_output/coverage-m.json && nyc report --reporter=lcov --report-dir=go-modules/.coverage --exclude '[]'",
    "report-coverage:go-modules": "comforter-cli --path ./go-modules/.coverage/lcov.info --name go-modules --branch $CI_COMMIT_REF_NAME --commit $CI_COMMIT_SHA ${CI_MERGE_REQUEST_IID:+--merge-request-iid $CI_MERGE_REQUEST_IID} ${CI_MERGE_REQUEST_DIFF_BASE_SHA:+--merge-base $CI_MERGE_REQUEST_DIFF_BASE_SHA} ${CI_MERGE_REQUEST_TARGET_BRANCH_NAME:+--target-branch $CI_MERGE_REQUEST_TARGET_BRANCH_NAME} --project $CI_PROJECT_ID --apiKey $GITLAB_API_KEY --zip ./go-modules/.coverage/lcov-report --host https://comforter.goreact.com",
    "report-coverage:ngx-go-modules": "comforter-cli --path ./angular/go-modules/.coverage/lcov.info --name ngx-go-modules --branch $CI_COMMIT_REF_NAME --commit $CI_COMMIT_SHA ${CI_MERGE_REQUEST_IID:+--merge-request-iid $CI_MERGE_REQUEST_IID} ${CI_MERGE_REQUEST_DIFF_BASE_SHA:+--merge-base $CI_MERGE_REQUEST_DIFF_BASE_SHA} ${CI_MERGE_REQUEST_TARGET_BRANCH_NAME:+--target-branch $CI_MERGE_REQUEST_TARGET_BRANCH_NAME} --project $CI_PROJECT_ID --apiKey $GITLAB_API_KEY --zip ./angular/go-modules/.coverage/lcov-report --host https://comforter.goreact.com",
    "report-coverage:lti": "comforter-cli --path ./lti/.coverage/lcov.info --name lti --branch $CI_COMMIT_REF_NAME --commit $CI_COMMIT_SHA ${CI_MERGE_REQUEST_IID:+--merge-request-iid $CI_MERGE_REQUEST_IID} ${CI_MERGE_REQUEST_DIFF_BASE_SHA:+--merge-base $CI_MERGE_REQUEST_DIFF_BASE_SHA} ${CI_MERGE_REQUEST_TARGET_BRANCH_NAME:+--target-branch $CI_MERGE_REQUEST_TARGET_BRANCH_NAME} --project $CI_PROJECT_ID --apiKey $GITLAB_API_KEY --zip ./lti/.coverage/lcov-report --host https://comforter.goreact.com",
    "report-coverage:ngx-lti": "comforter-cli --path ./angular/lti/.coverage/lcov.info --name ngx-lti --branch $CI_COMMIT_REF_NAME --commit $CI_COMMIT_SHA ${CI_MERGE_REQUEST_IID:+--merge-request-iid $CI_MERGE_REQUEST_IID} ${CI_MERGE_REQUEST_DIFF_BASE_SHA:+--merge-base $CI_MERGE_REQUEST_DIFF_BASE_SHA} ${CI_MERGE_REQUEST_TARGET_BRANCH_NAME:+--target-branch $CI_MERGE_REQUEST_TARGET_BRANCH_NAME} --project $CI_PROJECT_ID --apiKey $GITLAB_API_KEY --zip ./angular/lti/.coverage/lcov-report --host https://comforter.goreact.com",
    "test:wcag": "wcag ./dashboard && wcag ./go-modules && wcag ./lti",
    "htmllint": "htmlhint --config ./go-modules/.htmlhintrc --ignore goreact/**,angular/**,node_modules/** ./",
    "copy:settings": "cd ../htdocs/ && cp -n settings.example.js settings.js || true",
    "generate-test-data:lti": "gulp generate-test-data",
    "bundle:analyze": "webpack --progress --profile --json > stats.json && webpack-bundle-analyzer stats.json"
  },
